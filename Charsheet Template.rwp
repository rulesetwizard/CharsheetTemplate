function onInit()
	-- When any ability bonus changes we must update all the related bonuses (skills, AC, HP)
	DB.addHandler(DB.getPath(getDatabaseNode(), ".abilities.*.bonus"), "onUpdate", onAbilityBonusChanged);
end

function onAbilityBonusChanged(nodeAbility)
	skills.subwindow.UpdateSkills();
	combat.subwindow.AC.onSourceUpdate();
end</LUACode>
